{
  "name": "redux-examples",
  "version": "0.1.0",
  "description": "Examples for [Redux](http://redux.js.org).",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/redux-examples.git"
  },
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/redux-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/redux-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "eslint": "eslint . --ext .js,.jsx --cache",
    "eslint-fix": "eslint . --ext .js,.jsx --cache --fix",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,html,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,html,ts,tsx}'",
    "start": "node \"./Util - 01 - Custom servers/server.js\" 8040",
    "validate": "npm run eslint && npm run prettier"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-redux": "^7.0.9",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.4.0",
    "prettier": "^1.18.2",
    "redux-devtools": "^3.5.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "plugin-typescript": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-react-hook": "^3.3.2",
    "redux-thunk": "^2.3.0",
    "serve": "^11.0.1",
    "systemjs": "0.21.6",
    "systemjs-plugin-babel": "^0.0.25",
    "typescript": "^3.5.1"
  }
}
